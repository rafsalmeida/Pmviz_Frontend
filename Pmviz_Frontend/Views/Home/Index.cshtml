@{
    ViewData["Title"] = "Home Page";
}
@if (ViewBag.Error != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewBag.Error
    </div>
}

@if (ViewBag.Success != null)
{
    <div class="alert alert-success" role="alert">
        @ViewBag.Success
    </div>
}

<!-- start page title -->

<div class="row">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item active">Contactos</li>
                </ol>
            </div>
            <h4 class="page-title">Home</h4>
        </div>
    </div>
</div>

<!-- end page title -->


<!-- end row -->
<div class="row">
    <div class="col-lg-4">
        <div class="text-center card-box">
            <div class="pt-2 pb-2">
                <img src="../assets/images/users/user-3.jpg" class="rounded-circle img-thumbnail avatar-xl" alt="profile-image">
                <h4 class="mt-3"><a href="extras-profile.html" class="text-dark">Paulo Pereira</a></h4>
                <p class="text-muted">Developer <span> | </span> <span> <a href="#" class="text-pink">websitename.com</a> </span></p>
            </div> <!-- end .padding -->
        </div> <!-- end card-box-->
    </div> <!-- end col -->

    <div class="col-lg-4">
        <div class="text-center card-box">
            <div class="pt-2 pb-2">
                <img src="../assets/images/users/user-4.jpg" class="rounded-circle img-thumbnail avatar-xl" alt="profile-image">
                <h4 class="mt-3"><a href="extras-profile.html" class="text-dark">Rafaela Almeida</a></h4>
                <p class="text-muted">Developer <span> | </span> <span> <a href="https://rafsalmeida.github.io" class="text-pink">Website</a> </span></p>
            </div> <!-- end .padding -->
        </div> <!-- end card-box-->
    </div> <!-- end col -->
</div>